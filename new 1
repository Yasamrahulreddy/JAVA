import java.util.*;
public class Source {

   public static void main(String[] args) {
       int X,Y;
Scanner input= new Scanner(System.in);
X=input.nextInt();
Y=input.nextInt();
int output =X+Y;
if (output>30) {
System.out.println("The statement said by the person is true");
}
else {
System.out.println("The statement said by the person is false");
}
        }
}
